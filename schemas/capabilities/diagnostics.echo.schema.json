{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "diagnostics.echo.schema.json",
  "title": "Diagnostics Echo Capability",
  "description": "Capability for echoing text for diagnostic purposes",
  "type": "object",
  "required": ["text"],
  "properties": {
    "text": {
      "type": "string",
      "description": "Text to echo back"
    }
  },
  "additionalProperties": false,
  "examples": [
    {
      "text": "Hello world"
    }
  ]
}